<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.finalprojectnavtest.ui.Calendar.CalendarFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_calendar"
        />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.finalprojectnavtest.ui.AddNote.AddNoteFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_addnote" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.finalprojectnavtest.ui.Category.CategoryFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_category" />

    <fragment
        android:id="@+id/settings"
        android:name="com.example.finalprojectnavtest.ui.Settings.Settings"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
</navigation>